{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "export": "next build && next export",
    "netlify-build": "npm run build",
    "electron": "ELECTRON=true next build && electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "ELECTRON=true next build && electron-builder",
    "electron:build:win": "ELECTRON=true next build && electron-builder --win",
    "electron:build:mac": "ELECTRON=true next build && electron-builder --mac",
    "electron:build:linux": "ELECTRON=true next build && electron-builder --linux",
    "capacitor:init": "npx cap init",
    "capacitor:add:android": "npx cap add android",
    "capacitor:sync": "npm run build && npx cap sync",
    "capacitor:open:android": "npx cap open android",
    "capacitor:build:android": "npm run build && npx cap sync && npx cap open android"
  },
